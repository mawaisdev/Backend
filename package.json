{
  "name": "typeorm-sample",
  "version": "0.0.1",
  "description": "Awesome project developed with TypeORM.",
  "type": "commonjs",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/config": "^3.3.3",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/luxon": "^3.3.2",
    "@types/morgan": "^1.9.5",
    "@types/node": "^16.18.66",
    "@types/showdown": "^2.0.1",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "apollo-server": "^3.13.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "config": "^3.3.9",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.17.2",
    "graphql": "^15.8.0",
    "jsonwebtoken": "^9.0.2",
    "luxon": "^3.4.3",
    "morgan": "^1.10.0",
    "nanoid": "^5.0.3",
    "pg": "^8.4.0",
    "reflect-metadata": "^0.1.13",
    "showdown": "^2.1.0",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.3.17"
  },
  "scripts": {
    "start": "ts-node-dev --respawn --transpile-only src/index.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "dev": "nodemon",
    "migrate:gen": "npx typeorm-ts-node-esm migration:generate ./src/Migration/Migration-Name -d ./src/data-source.ts",
    "migrate:run": "npx typeorm-ts-node-esm migration:run -d ./src/data-source.ts"
  }
}
